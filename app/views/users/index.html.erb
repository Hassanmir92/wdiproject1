<h1>Warriors!</h1>

<table>
  <thead>
    <tr>
      <th> </th>
      <th>Username</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td class="profile_image_thumb"><%= image_tag user.user_image.thumb %></td>
        <td><%= link_to "#{user.username}", user %></td>
      </tr>
    <% end %>
  </tbody>
</table>

